<%Refinery::PvFaqs::PvFaq.all.each_with_index do |faq,index|%>
	<div class="row_lm">
		<div class="body_text_2 pad_r_4_900_st">
			<div class="unselected container_collapse" id="row_<%= index%>">
				<div class="light c_gray_pro_1 block pad_b_20 action_collapse split_border" onclick="javascript:unhide('row_<%= index%>');">
					<div class="fs_18 c_green_1 lh_2 pad_b_1">
						<%=raw faq.question%>
					</div>
				</div>
				<div class="collpase_hide">
					<br>
					<%=raw faq.answer%>
				</div>
			</div>
		</div>
	</div>
<% end %>
