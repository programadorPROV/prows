<%= render 'refinery/admin/search_header', :url => refinery.pv_home_main_sections_admin_pv_home_main_sections_path %>
<div class='pagination_container'>
  <% if @pv_home_main_sections.any? %>
    <%= render 'pv_home_main_sections' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
</div>
